@model TesisProj.Areas.Simulaciones.Models.ModeloSimulacion
@using Telerik.Web.Mvc.UI;
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
    String[] Cabeceras = (string[])ViewBag.Cabeceras;
    String[] Valores = (string[])ViewBag.Valores;
    String[] CronogramaTitulo = (string[])ViewBag.CronogramaTitulo;
    String[] CronogramaValor = (string[])ViewBag.CronogramaValor;
    String[] ValoresSimulados = (string[])ViewBag.ValoresSimulados;
}

<h2>@ViewBag.Titulo</h2>
<b>Valores Reales</b>
<p>
    <br />
    <a class="btn btn-inverse" style="color: #E6F0F2" href="@Url.Action("Index", "ParametrosSencibles", new { idProyecto = ViewBag.ProyectoId  })"">
  	    <i class="icon-arrow-left icon-white"></i>Regresar
    </a>
    <br /> <br />
</p>

    <table class="table table-hover table-striped table-bordered">
            <tr class="success">
                @foreach (string s in CronogramaTitulo)
                {
                    <td><u><b>@s</b></u></td>
                }
            </tr>
            <tr>
                @foreach(string s in CronogramaValor)
                {
                    <td><u><b>@s</b></u></td>
                }
            </tr>
        </table>
    
    @if (ValoresSimulados != null)
    {
        <b>Valores Simulados</b>
        <table class="table table-hover table-striped table-bordered">
            <tr class="success">
                @foreach (string s in CronogramaTitulo)
                {
                    <td><u><b>@s</b></u></td>
                }
            </tr>
            <tr>
                @foreach (string s in ValoresSimulados)
                {
                    <td><u><b>@s</b></u></td>
                }
            </tr>
        </table>
    }

    @if(Cabeceras!=null)
    {
        <h3><b>@ViewBag.SubTitulo</b></h3>
        <b>Parametros</b>
        <table class="table table-hover table-striped table-bordered">
            <tr class="success">
                @foreach (string s in Cabeceras)
                {
                    <td><u><b>@s</b></u></td>
                }
            </tr>
            <tr>
                @foreach(string s in Valores)
                {
                    <td><u><b>@s</b></u></td>
                }
            </tr>
        </table>
        if (Session["Parche"] != null)
        {<table class="table table-hover table-striped table-bordered">
            <tr>
                <td>Valores Ingresados</td>
                <td>@(Server.HtmlEncode(((string)Session["Parche"])))</td>
            </tr>
         </table>
        }
    }
    


