@using Telerik.Web.Mvc.UI
@using TesisProj.Areas.Proyectos.Models
@using TesisProj.Areas.Modelo.Models 
@model ICollection<TesisProj.Areas.Modelo.Models.Celda>

<table>
    <tr>
        @foreach (Celda e in Model)
        {
            <th>@e.Periodo</th>
        }
    </tr>
    <tr>
        @foreach (Celda e in Model)
        {
            <td>@(Math.Round(e.Valor,2))</td>
        }
    </tr>
</table>