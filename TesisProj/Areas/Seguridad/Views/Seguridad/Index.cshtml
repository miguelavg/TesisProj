@model IEnumerable<TesisProj.Areas.Modelo.Models.Proyecto>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Creacion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdCreador)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Horizonte)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PeriodosPreOp)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PeriodosCierre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Version)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Creacion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IdCreador)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Horizonte)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PeriodosPreOp)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PeriodosCierre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Version)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
