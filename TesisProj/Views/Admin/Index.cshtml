<table class="table table-hover table-striped table-bordered table-condensed">
    <tr>
        <td>
            <ol class="round">
             <li class="one">
                    <h5><a href="@Url.Action("Index", "TipoElemento", new { Area = "Plantilla" })">Tipos de elemento</a></h5>
                    Administrar tipos de los elementos
                </li>
            </ol>
        </td>
    </tr>
    <tr>
        <td>
            <ol class="round">
                <li class="two">
                    <h5><a href="@Url.Action("Index", "TipoFormula", new { Area = "Plantilla" })">Tipo de fórmulas</a></h5>
                    Administrar tipos de las fórmulas
                </li>
            </ol>
        </td>
    </tr>
    <tr>
        <td>
            <ol class="round">
                <li class="three">
                    <h5><a href="@Url.Action("Index", "TipoParametro", new { Area = "Plantilla" })">Tipos de parámetro</a></h5>
                    Administrar tipos de los parámetros
                </li>
            </ol>
