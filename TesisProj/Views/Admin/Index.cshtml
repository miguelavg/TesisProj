@{
    ViewBag.Title = "Evaluación de proyectos";
}
@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@ViewBag.Title.</h1>
                <h2>@ViewBag.Message</h2>
            </hgroup>
            <p>
                ¡Aquí vamos!
            </p>
        </div>
    </section>
}
<h3>Administración:</h3>
<ol class="round">
    <li class="one">
        <h5><a href="@Url.Action("Index", "TipoElemento", new { Area = "Plantilla" })">Tipos de elemento</a></h5>
        Administrar tipos de los elementos
    </li>
    <li class="two">
        <h5><a href="@Url.Action("Index", "TipoFormula", new { Area = "Plantilla" })">Tipo de fórmulas</a></h5>
        Administrar tipos de las fórmulas
    </li>
    <li class="three">
        <h5><a href="@Url.Action("Index", "TipoParametro", new { Area = "Plantilla" })">Tipos de parámetro</a></h5>
        Administrar tipos de los parámetros
    </li>
    <li class="four">
        <h5><a href="@Url.Action("Index", "PlantillaElemento", new { Area = "Plantilla" })">Plantillas de elementos</a></h5>
        Administrar las plantillas de los elementos
    </li>
    <li class="five">
        <h5><a href="@Url.Action("Index", "PlantillaProyecto", new { Area = "Plantilla" })">Plantillas de proyectos</a></h5>
        Administrar las plantillas de los proyectos
    </li>
</ol>
